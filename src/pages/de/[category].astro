---
import CategoryLandingLayout from "@layouts/CategoryLandingLayout.astro";
import { getCategoryPageContents } from "~/utils/getYamlContent";



export function getStaticPaths() {
  return [{ params: { category: "learn" } }];
}


const { category } = Astro.params;
const CategoryData = getCategoryPageContents(`de/${category}`);
---

<CategoryLandingLayout category={category} data={CategoryData} />
