---
import CategoryLandingLayout from "@layouts/CategoryLandingLayout.astro";

import { getCategoryPageContents } from "~/utils/getYamlContent";


export function getStaticPaths() {
  return [
    { params: { category: "company" } },
    { params: { category: "contact" } },
    { params: { category: "email" } },
    { params: { category: "diary" } },
    { params: { category: "api" } },
    { params: { category: "database" } },
    { params: { category: "learn" } },
    { params: { category: "online" } },
    { params: { category: "onsite" } },
    { params: { category: "service" } },
  ];
}


const { category } = Astro.params;
const CategoryData = getCategoryPageContents(`en/${category}`);
---

<CategoryLandingLayout data={CategoryData} category={category} />
