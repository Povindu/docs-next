---
import SubCategoryLandingLayout from "@layouts/SubCategoryLandingLayout.astro";
import { getTableOfContents } from "~/utils/getTableOfContents";

export function getStaticPaths() {
  return [
    { params: { subcategory: "document" } },
    { params: { subcategory: "project" } },
    { params: { subcategory: "request" } },
    { params: { subcategory: "sale" } },
    { params: { subcategory: "mobile" } },
    { params: { subcategory: "developer-portal" } },
    { params: { subcategory: "automation" } },
  ];
}

import { getSubCategoryPageContents } from "~/utils/getYamlContent";

const { subcategory } = Astro.params;
const SubCategoryData = getSubCategoryPageContents(`en/${subcategory}`);
const ToCData = getTableOfContents(`docs/en/${subcategory}`, "/toc", false);
---

<SubCategoryLandingLayout data={SubCategoryData} ToCData={ToCData} />
